#include <iostream>

using namespace std;

int main()
{
    int n, d;
    int count = 0;
    cin >> n >> d;
    if (n%d != 0){
        do{
            count ++;
        }while((n+count)%d!=0);
    cout << (count+n)/d << endl;
    cout << "SOBRA " << count << endl;
    }else{
        cout << n/d << endl;
    }
    
    return 0;
}
